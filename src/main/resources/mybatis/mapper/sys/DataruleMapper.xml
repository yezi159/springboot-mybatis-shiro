<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liuk.springboot.mapper.DataruleMapper">

    <!-- 开启二级缓存 -->
    <!--<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>-->

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.liuk.springboot.entity.Datarule">
        <id column="id" property="id" />
        <result column="menu_id" property="menuId" />
        <result column="name" property="name" />
        <result column="t_field" property="tField" />
        <result column="t_express" property="tExpress" />
        <result column="t_value" property="tValue" />
        <result column="sql_segment" property="sqlSegment" />
        <result column="remarks" property="remarks" />
        <result column="del_flag" property="delFlag" />
        <result column="className" property="className" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, menu_id, name, t_field, t_express, t_value, sql_segment, remarks, del_flag, className
    </sql>

</mapper>
