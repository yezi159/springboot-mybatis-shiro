<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liuk.springboot.mapper.ConfigMapper">

    <!-- 开启二级缓存 -->
    <!--<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>-->

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.liuk.springboot.entity.Config">
        <id column="id" property="id" />
        <result column="smtp" property="smtp" />
        <result column="port" property="port" />
        <result column="mailname" property="mailname" />
        <result column="mailpassword" property="mailpassword" />
        <result column="smsname" property="smsname" />
        <result column="smspassword" property="smspassword" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, smtp, port, mailname, mailpassword, smsname, smspassword
    </sql>

</mapper>
